gau(){
	tools/gau/./main $1
}
gau_s(){
	tools/gau/./main --subs $1
}

ffuf(){
	tools/ffuf/./main -u $1 -w $2 -b $3 -c -t 100 
}
openredirex(){
	python3 tools/OpenRedireX/openredirex.py -l $1 -p $2
}
qsreplace(){
	tools/qsreplace/./main $1
